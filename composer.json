{
    "name": "mpprogramming/context",
    "version": "1.0.0",
    "type": "library",
    "description": "Library to filter HTTP request responses by context.",
    "keywords": [
        "context",
        "laravel",
        "filters"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "MP Programming",
            "email":"mp.programming007@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "astrotomic/laravel-translatable": "^11.10",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^0.2.3",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Context\\": "src/",
            "Context\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Context\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "lint": "./vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Context\\ContextServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}